@model IEnumerable<WebSiteBanHang.Models.ThanhVien>

@{
    ViewBag.Title = "GroupDuLieu";
}

<h2>GroupDuLieu</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
@foreach (var Group in Model.GroupBy(n => n.MaLoaiTV))
{
    <div>@Group.Key</div>
    foreach (var item in Group)
    {
        <p>@item.TaiKhoan</p>
    }

}
